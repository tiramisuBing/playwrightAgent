{
  "name": "playwright-test-agent-framework",
  "version": "1.0.0",
  "description": "Playwright Test Agent Framework - 自动化测试生成和维护框架",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "report": "playwright show-report",
    "init-agents": "playwright init-agents --loop=vscode",
    "init-agents:claude": "playwright init-agents --loop=claude",
    "init-agents:opencode": "playwright init-agents --loop=opencode"
  },
  "keywords": [
    "playwright",
    "test-agent",
    "automation",
    "testing",
    "ai",
    "self-healing"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
